{
    "files.associations": {
        "*.tidal": "haskell"
    },

    "tidalcycles.ghciPath" : "ghci",
    "tidalcycles.feedbackColor": "rgba(100,250,100,0.5)",
    "tidalcycles.useStackGhci": false,
