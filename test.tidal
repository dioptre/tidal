-- PUNCHY DRUM COMPRESSION - Aggressive attack
d2 $ s "bd*4"
  # punchyCompress         -- Preset: fast attack, 4:1 ratio, makeup gain

-- HEAVY LIMITING - Prevent clipping on loud synths
d3 $ note "c a f e" # s "superpiano" # gain 1.5
  # limit 0.8             -- 80% wet
  # limitthresh 0.9       -- Limit above 90%
  # limitrelease 0.01     -- Fast release

