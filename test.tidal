-- #limit (brick wall), #multiband (frequency-specific)
-- All use wetness/dryness architecture like convolution reverb

-- BASIC COMPRESSION - Add punch to drums
d1 $ s "bd*4 sn*2"
  # compress 0.5           -- 50% wet/dry mix
  # compressratio 4        -- 4:1 compression ratio
  # compressthresh 0.5     -- Medium threshold
  # compressmakeup 1.3     -- Boost output 30%

hush

-- GENTLE "GLUE" COMPRESSION - Mix bus style
